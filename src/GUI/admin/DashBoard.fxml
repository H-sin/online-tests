            </ImageView>

                <JFXButton fx:id="btnConsole" onAction="#switchConsole" prefHeight="42.0" prefWidth="162.0" text="Console" textFill="#a1aec4">
                    <font>
                        <Font name="Roboto Bold" size="14.0" />
                    </font>
                </JFXButton>

                <JFXButton fx:id="btnEtudiants" layoutX="10.0" layoutY="67.0" onAction="#switchEtudiants" prefHeight="42.0" prefWidth="164.0" text="Etudiants" textFill="#a1aec4">
